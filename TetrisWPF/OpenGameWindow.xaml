<Window x:Class="TetrisWPF.OpenGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TetrisWPF"
        mc:Ignorable="d"
        Title="Open Game" Height="450" Width="400">
    <Grid>
        <DataGrid x:Name="Table" Margin="10" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.ColumnSpan="3"  MouseDoubleClick="Control_OnMouseDoubleClick"></DataGrid>
    </Grid>
</Window>
